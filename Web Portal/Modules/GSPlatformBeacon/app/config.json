{
    "id" : "beacon",
    "name" : "Beacon",
    "module" : "beacon",
    "version" : "1.0",
    "nav" : "views/templates/navMenu.html",
    "desc" : "A dashboard to monitor code analyze results",
    "serviceUrl" : "http://nkgbeacon.eng.citrite.net/api/1",
    "routers" : [{
            "name" : "product",
            "url" : "/",
            "templateUrl": "views/product.html",
            "script" : "scripts/product/product.js"
        },{
            "name" : "revisions",
            "url"  : "/revisions/:repo",
            "controller" : "revisionsRouteCtrl",
            "templateUrl": "views/revisions.html",
            "script" : "scripts/revisions/revisions.js",
            "children" : [
                {
                    "name" : "revision",
                    "url" : "/revision/:rev",
                    "controller" : "revisionRouteCtrl",
                    "templateUrl": "scripts/revisions/revision/templates/revision.html",
                    "script" : "scripts/revisions/revision/revision.js"
                }]
        },{
            "name" : "settings",
            "templateUrl": "views/settings.html",
            "script" : "scripts/settings/settings.js"
        }]
}