{
    "id" : "beacon",
    "name" : "Beacon",
    "module" : "beacon",
    "version" : "1.0",
    "nav" : "views/templates/navMenu.html",
    "desc" : "A dashboard to monitor code analyze results",
    "serviceUrl" : "http://nkgbeacon.eng.citrite.net/api/1",
    "routers" : [{
            "name" : "product",
            "url" : "/",
            "templateUrl": "views/product.html",
            "script" : "product/product.js"
        },{
            "name" : "revisions",
            "url"  : "/revisions/:repo",
            "controller" : "beacon.revisionsRouteCtrl",
            "templateUrl": "views/revisions.html",
            "script" : "revisions/revisions.js",
            "children" : [{
                "name" : "revision",
                "url" : "/revision/:rev",
                "controller" : "beacon.revisionCtrl",
                "templateUrl": "scripts/revisions/revision/templates/revision.html",
                "script" : "revisions/revision/revision.js"
            }]
        },{
            "name" : "settings",
            "views" : {
                "" : {
                    "templateUrl": "views/settings.html"
                },
                "predefined@settings" : {
                    "templateUrl": "scripts/settings/templates/predefinedTab.html"
                },
                "customized@settings" : {
                    "templateUrl": "scripts/settings/templates/customizedTab.html"
                }
            },
            "script" : "settings/settings.js"
        }]
}